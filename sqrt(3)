class Solution {
public:
    int mySqrt(int x) {
        long i=1;
        while(i*i<=x){
            i++;
        }
        if(i*i==x){
            return i;

        }
        else if(i*i>x){
            return i-1;
        
        }
        else{
            return 0;
        }
        

    }
};
